{
    "apiVersion": "appconnect.ibm.com/v1beta1",
    "kind": "IntegrationServer",
    "metadata": {
        "name": "name",
        "namespace": "namespace"
    },
    "spec": {
        "barURL": "",
        "designerFlowsOperationMode": "disabled",
        "license": {
            "accept": true,
            "license": "L-AMYG-BQ2E4U",
            "use": "AppConnectEnterpriseProduction"
        },
        "pod": {
            "containers": {
                "runtime": {
                    "image": "image",
                    "resources" : {
                        "requests" : {
                            "cpu" : "250m",
                            "memory" : "256Mi"
                        },
                        "limits" : {
                            "cpu" : "500m",
                            "memory" : "512Mi"
                        }
                    }
                }
            },
            "imagePullSecrets": [
                {
                    "name": "all-icr-io"
                }
            ]
        },
        "replicas": 1,
        "router": {
            "timeout": "120s"
        },
        "service": {
            "endpointType": "https"
        },
        "tracing": {
            "enabled": false
        },
        "useCommonServices": false,
        "version": "11.0.0"
    }
}
