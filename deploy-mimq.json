{
    "apiVersion": "mq.ibm.com/v1beta1",
    "kind": "QueueManager",
    "metadata": {
        "name": "name",
        "namespace": "namespace"
     },
    "spec": {
        "affinity": {
            "nodeAffinity": {
                "preferredDuringSchedulingIgnoredDuringExecution": [
                    {
                        "preference": {
                            "matchExpressions": [
                                {
                                    "key": "qmworkernode",
                                    "operator": "In",
                                    "values": [
                                        "3"
                                    ]
                                }
                            ]
                        },
                        "weight": 1
                    }
                ],
                "requiredDuringSchedulingIgnoredDuringExecution": {
                    "nodeSelectorTerms": [
                        {
                            "matchExpressions": [
                                {
                                    "key": "qmworkernode",
                                    "operator": "In",
                                    "values": [
                                        "3",
                                        "2"
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "imagePullSecrets": [
            {
                "name": "all-icr-io"
            }
        ],
        "license": {
            "accept": true,
            "license": "L-RJON-BN7PN3",
            "metric": "VirtualProcessorCore",
            "use": "Production"
        },
        "queueManager": {
            "availability": {
                "type": "MultiInstance"
            },
            "image": "image",
            "imagePullPolicy": "IfNotPresent",
            "livenessProbe": {
                "failureThreshold": 1,
                "initialDelaySeconds": 420,
                "periodSeconds": 10,
                "successThreshold": 1,
                "timeoutSeconds": 5
            },			
			"metrics": {
                "enabled": true
            },
            "name": "QM1",
            "readinessProbe": {
                "failureThreshold": 1,
                "initialDelaySeconds": 300,
                "periodSeconds": 5,
                "successThreshold": 1,
                "timeoutSeconds": 3
            },			
            "resources": {
                "limits": {
                    "cpu": "500m",
                    "memory": "512Mi"
                },
                "requests": {
                    "cpu": "250m",
                    "memory": "256Mi"
                }
            },
            "route": {
                "enabled": false
            },
            "storage": {
                "persistedData": {
                    "class": "ibmc-file-gold-gid",
                    "enabled": true,
                    "size": "20Gi",
                    "type": "persistent-claim"
                },
                "queueManager": {
                    "class": "ibmc-file-gold-gid",
                    "size": "20Gi",
                    "type": "persistent-claim"
                },
                "recoveryLogs": {
                    "class": "ibmc-file-gold-gid",
                    "enabled": true,
                    "size": "20Gi",
                    "type": "persistent-claim"
                }
            }
        },
        "securityContext": {
            "supplementalGroups": [
                99
            ]
        },
        "template": {
            "pod": {
                "containers": [
                    {
                        "env": [
                            {
                                "name": "MQSNOAUT",
                                "value": "yes"
                            }
                        ],
                        "name": "qmgr"
                    }
                ]
            }
        },
        "version": "9.2.0.0-r1",
        "web": {
            "enabled": false
        }
    }
 }
